{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "PYTHON"
  },
  "deploy": {
    "startCommand": "python main.py",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 3,
    "numReplicas": 1,
    "healthcheckPath": null,
    "healthcheckTimeout": null
  }
} 